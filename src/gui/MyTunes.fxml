<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MyTunesController">
   <children>
      <ListView fx:id="lvPlaylistSongs" layoutX="246.0" layoutY="92.0" prefHeight="382.0" prefWidth="243.0" />
      <Label layoutX="38.0" layoutY="75.0" text="Playlists" />
      <TableView fx:id="tvPlaylists" layoutX="28.0" layoutY="92.0" prefHeight="382.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tcPlaylistName" prefWidth="88.0" text="Name" />
          <TableColumn fx:id="tcNumberSongs" prefWidth="47.0" text="Songs" />
            <TableColumn fx:id="tcPlaylistTime" prefWidth="64.0" text="Time" />
        </columns>
      </TableView>
      <TableView fx:id="tvSongTable" layoutX="507.0" layoutY="92.0" prefHeight="382.0" prefWidth="364.0">
         <columns>
            <TableColumn fx:id="tcSongTitle" prefWidth="143.0" text="Title" />
            <TableColumn fx:id="tcSongArtsist" prefWidth="145.0" text="Artist" />
            <TableColumn fx:id="tcSongTime" prefWidth="75.0" text="Time" />
         </columns>
      </TableView>
      <TextField fx:id="txtSearchBar" layoutX="689.0" layoutY="34.0" prefHeight="25.0" prefWidth="106.0" />
      <Label layoutX="648.0" layoutY="38.0" text="Filter:" />
      <Button layoutX="820.0" layoutY="34.0" mnemonicParsing="false" onAction="#Search" prefHeight="25.0" text="search" />
      <Label fx:id="lblNowPlaying" layoutX="331.0" layoutY="46.0" text="Something is playing">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="246.0" layoutY="75.0" text="Songs on Playlist" />
      <Label layoutX="507.0" layoutY="75.0" text="Songs" />
      <Button layoutX="507.0" layoutY="485.0" mnemonicParsing="false" onAction="#AddToPlaylist" prefHeight="26.0" text="Add To Playlist" />
      <Button layoutX="28.0" layoutY="485.0" mnemonicParsing="false" onAction="#NewPlaylist" prefHeight="25.0" prefWidth="55.0" text="New" />
      <Button layoutX="102.0" layoutY="485.0" mnemonicParsing="false" onAction="#UpdatePlaylist" text="Update" />
      <Button layoutX="172.0" layoutY="485.0" mnemonicParsing="false" onAction="#DeletePlaylist" prefHeight="25.0" prefWidth="55.0" text="Delete" />
      <Button layoutX="246.0" layoutY="485.0" mnemonicParsing="false" onAction="#PositionUp" prefHeight="25.0" prefWidth="55.0" text="Up" />
      <Button layoutX="309.0" layoutY="485.0" mnemonicParsing="false" onAction="#PositionDown" prefHeight="25.0" prefWidth="55.0" text="Down" />
      <Button layoutX="414.0" layoutY="485.0" mnemonicParsing="false" onAction="#RemoveFromPlaylist" prefHeight="25.0" prefWidth="55.0" text="Delete" />
      <Button layoutX="677.0" layoutY="485.0" mnemonicParsing="false" onAction="#NewSong" prefHeight="25.0" prefWidth="55.0" text="New" />
      <Button layoutX="742.0" layoutY="485.0" mnemonicParsing="false" onAction="#EditSong" prefHeight="25.0" prefWidth="55.0" text="Edit" />
      <Button layoutX="816.0" layoutY="485.0" mnemonicParsing="false" onAction="#DeleteSong" prefHeight="25.0" prefWidth="55.0" text="Delete" />
   </children>
</AnchorPane>
